<?xml version="1.0" encoding="utf-8" ?>  
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Practice" namespace="Practice">  
  <class name="Student" table="Student" dynamic-update="true" xmlns="urn:nhibernate-mapping-2.2">    
    <id name="ID" column="Id" type="int">  
      <generator class="identity" />  
    </id>  
    <property name="FirstName" column="FirstName"/>  
    <property name="MiddleName" column="MiddleName"/>    
    <property name="LastName" column="LastName"/>  
    <property name="GroupNumber" column="GroupNumber"/>  
    <property name="Email" column="Email"/>   
   </class>  
</hibernate-mapping> 